<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
<head>
    <% include ./partials/header-layout.ejs%>
</head>
<body>
<header>
    <!-- Navigation -->
    <% include ./partials/header-product.ejs%>

    <div class="header-content">
        <div class="header-content-inner">
            <div class="col-md-8 col-md-offset-2">
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li class="active" data-target="#myCarousel" data-sldie-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                        <li data-target="#myCarousel" data-slide-to="3"></li>
                        <li data-target="#myCarousel" data-slide-to="4"></li>
                    </ol>
                    <div class="carousel-inner" role="listbox">
                        <div class="item active">
                            <img src="/images/product/WisataAlam" style="width:100%">
                            <a href="/product/WisataAlam">Wisata Alam</a>
                        </div>
                        <%coursels.forEach((coursel)=>{%>
                        <div class="item">
                            <img src="/images/product/<%=coursel.url%>" style="width:100%">
                            <a href="/product/<%=coursel.href%>"><%= coursel.namakategori%></a>
                        </div>
                        <%});%>
                    </div>
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                          <span class="glyphicon glyphicon-chevron-left">
                              <span class="sr-only">Previous</span>
                          </span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                          <span class="glyphicon glyphicon-chevron-right">
                              <span class="sr-only">Next</span>
                          </span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</header>
<main>

</main>
<footer class="footer-distributed">
    <% include ./partials/footer.ejs%>
</footer>
</body>
</html>
